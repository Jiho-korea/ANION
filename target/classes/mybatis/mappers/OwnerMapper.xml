<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="petProject.dao.OwnerDAO">
    <select id="selectByIdPassword" parameterType="loginRequest" resultType="owner">
    	SELECT "OWNER_ID", "OWNER_PASSWORD" FROM "owner" WHERE "OWNER_ID" = #{loginRequest.id} AND "OWNER_PASSWORD" = #{loginRequest.password}
    </select>
</mapper>
