<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="petProject.dao.WithdrawalDAO">
	<insert id="insertComment" parameterType="withdrawalRequest">
		INSERT INTO "withdrawal"("MEMBER_NUMBER", "COMMENT", "WITHDRAWAL_DATE")
		VALUES (#{withdrawalRequest.memberNumber}, #{withdrawalRequest.comment}, now())
	</insert>
</mapper>
