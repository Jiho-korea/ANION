<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="petProject.dao.ImageDAO">
	<insert id="insertImage" parameterType="imageUploadRequest">
		INSERT INTO public.image
		("PET_REGISTRATION_NUMBER", "IMAGE_PATH", "OWNER_ID")
		VALUES(#{imageUploadRequest.petRegistrationNumber}, #{imageUploadRequest.imagePath}, #{imageUploadRequest.ownerId});
	</insert>
</mapper>
