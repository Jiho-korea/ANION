<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="petProject.dao.ImgpostAndImageDAO">
	<insert id="insertImgpostAndImage"
		parameterType="imgpostAndImage">
		INSERT INTO public.imgpost_and_image
		("IMGPOST_NO", "IMAGE_NUMBER")
		VALUES(#{imgpostAndImage.imgpostNo}, #{imgpostAndImage.imageNumber});
	</insert>
</mapper>
